{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "rebaseWhen": "conflicted",
  "labels": [
    "dependencies"
  ],
  "prConcurrentLimit": 3,
  "lockFileMaintenance": {
    "enabled": true
  },
  "commitMessagePrefix": "chore(deps): "
}
